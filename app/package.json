{
  "name": "QBaseCron",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run product",
    "product": "sudo pm2 start bin/www -f -x -i max --name push",
    "dev": "./node_modules/.bin/supervisor ./bin/www",
    "test": "./node_modules/.bin/mocha --reporter spec",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot",
    "test-travis": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter dot"
  },
  "dependencies": {
    "body-parser": "~1.8.1",
    "cookie-parser": "~1.3.3",
    "debug": "~2.0.0",
    "express": "~4.9.0",
    "jade": "~1.6.0",
    "moment": "^2.8.3",
    "mongodb": "^1.4.19",
    "mongoose": "^3.8.17",
    "monk": "^0.9.0",
    "morgan": "~1.3.0",
    "msgpack": "^0.2.4",
    "redis": "^0.12.1",
    "request": "^2.45.0",
    "serve-favicon": "^2.1.5"
  },
  "devDependencies": {
    "chai": "^1.9.2",
    "istanbul": "^0.3.2",
    "mocha": "^1.21.5",
    "sinon": "^1.10.3",
    "supervisor": "^0.6.0"
  }
}
